<!DOCTYPE html>
<html>
<head>
  <title>My first Vue app</title>
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
</head>
<body>
  <div id="app">
    <input v-on:input="vall = $event.target.value" type="text" />
    
  <ul id="zz">      
      <li>{{vall}} * 1 = {{vall * 1}}</li>
      <li>{{vall}} * 2 = {{vall * 2}}</li>
      <li>{{vall}} * 3 = {{vall * 3}}</li>
      <li>{{vall}} * 4 = {{vall * 4}}</li>
      <li>{{vall}} * 5 = {{vall * 5}}</li>
      <li>{{vall}} * 6 = {{vall * 6}}</li>
      <li>{{vall}} * 7 = {{vall * 7}}</li>
      <li>{{vall}} * 8 = {{vall * 8}}</li>
      <li>{{vall}} * 9 = {{vall * 9}}</li>
      <li>{{vall}} * 10 = {{vall * 10}}</li>
  </ul>
  </div>

  <script>
    var app = new Vue({
      el: '#app',
      data: {
        vall: '0'
      }
    })
  </script>
</body>
</html>
